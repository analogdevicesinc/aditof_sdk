[Tasks]
Name: "desktopicon"; Description: "{cm:CreateDesktopIcon}"; GroupDescription: "{cm:AdditionalIcons}"

[Files]
Source: "C:\projects\aditof-sdk\build\sdk\Release\aditof.dll"; DestDir: "{app}"
Source: "C:\projects\aditof-sdk\build\examples\aditof-demo\Release\aditof-demo.exe"; DestDir: "{app}";
Source: "C:\tools\opencv\build\x64\vc15\bin\opencv_world410.dll"; DestDir: "{app}";
